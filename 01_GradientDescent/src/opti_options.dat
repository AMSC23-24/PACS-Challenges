# Optimization options
[optimization]
tol_res = 1.e-6
tol_step_length = 1.e-6
max_iter = 1000
opti_verbose = false

# Options for the choice of the step size algorithm
[step_size]
option = Armijo
step_size_verbose = false

# parameters for the methods
alpha_0 = 1
[./armijo]
sigma = 0.45
[./exponential]
mu = 0.2
[./inverse_decay]
mu = 0.2

# Gradient computation method